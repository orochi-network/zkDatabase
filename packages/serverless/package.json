    "express": "^4.20.0",
    "express-fileupload": "^1.4.0",
    "google-auth-library": "^9.0.0",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0",
    "graphql-scalars": "^1.22.4",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.13.3",
    "jose": "^4.14.6",
    "mina-signer": "^3.0.4",
    "mongodb": "^6.3.0",
    "o1js": "^1.1.0",
    "redis": "^4.6.11",
    "uuid": "^9.0.1"
  }
