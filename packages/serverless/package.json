  "homepage": "https://github.com/orochi-network/zkDatabase#readme",
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "buildw": "tsc -p tsconfig.json --watch",
    "format": "prettier --write --ignore-unknown **/*",
    "dev": "npx nodemon -e ts -w ./src -x \"npx ts-node ./src/index.ts\"",
    "test": "node ../../node_modules/jest/bin/jest.js --runInBand",
    "testw": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint ./src/**/*.ts --fix && prettier . --write",
    "lint-check": "eslint ./src/**/*.ts --debug"
  },
  "lint-staged": {
    "**/*": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
