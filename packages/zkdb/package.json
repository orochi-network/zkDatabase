{
  "name": "zkdb",
  "version": "0.1.5",
  "description": "zkDatabase for the future of Web3",
  "author": "chiro@orochi.network",
  "license": "Apache-2.0",
  "keywords": [
    "zk",
    "database",
    "zk-database",
    "zkdb",
    "zkdatabase"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orochi-network/zkDatabase.git"
  },
  "bugs": {
    "url": "https://github.com/orochi-network/zkDatabase/issues"
  },
  "homepage": "https://github.com/orochi-network/zkDatabase#readme",
  "type": "module",
  "types": "./build/src/core/index.d.ts",
  "scripts": {
    "release": "rm -rf ./build && npm run build && npm publish --access public",
    "build": "tsc -p tsconfig.json",
    "buildw": "tsc -p tsconfig.json --watch",
    "coverage": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "prepare": "cd .. && cd .. && husky install packages/zkdb/.husky",
    "test": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js",
    "testw": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --watch",
    "lint": "npx eslint ./src/**/*.ts --fix",
    "lint-check": "npx eslint ./src/**/*.ts --debug"
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "build/src/*"
      ]
    }
  },
  "files": [
    "build/src",
    "!**/*.tsbuildinfo",
    "!**/*.test.js",
    "!**/*.test.js.map",
    "!**/*.test.d.ts",
    "!**/*.test.d.ts.map"
  ],
  "exports": {
    ".": {
      "types": "./build/src/core/index.d.ts",
      "import": "./build/src/core/index.js"
    },
    "./utilities": {
      "types": "./build/src/utilities/index.d.ts",
      "import": "./build/src/utilities/index.js"
    },
    "./index": {
      "types": "./build/src/index/index.d.ts",
      "import": "./build/src/index/index.js"
    },
    "./merkle-tree": {
      "types": "./build/src/merkle-tree/index.d.ts",
      "import": "./build/src/merkle-tree/index.js"
    },
    "./storage-engine": {
      "types": "./build/src/storage-engine/index.d.ts",
      "import": "./build/src/storage-engine/index.js"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5",
    "@types/cors": "^2.8.13",
    "@types/jest": "^27.5.2",
    "@types/safer-buffer": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-snarkyjs": "^0.1.0",
    "jest": "^27.5.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.8.8",
    "ts-jest": "^27.1.5",
    "typedoc": "^0.24.7",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@zkdb/kubo": "^1.0.3",
    "bson": "^5.3.0",
    "husky": "^8.0.3",
    "snarkyjs": "^0.11.4"
  }
}
