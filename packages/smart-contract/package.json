    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-o1js": "^0.4.0",
    "jest": "^28.1.3",
    "prettier": "^3.3.3",
    "rollup": "^4.24.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "o1js": "*"
  },
  "engines": {
    "node": ">=18.14.0"
  },
