  "keywords": [
    "mina-smart-contract",
    "zkdb",
    "zkdatabase"
  ],
  "type": "module",
  "main": "build/src/index.cjs.js",
  "module": "build/src/index.esm.js",
  "exports": {
    ".": {
      "import": "./build/src/index.esm.js",
      "require": "./build/src/index.cjs.js"
    }
  },
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "rm -rf ./build && rollup -c",
    "buildw": "tsc --watch",
    "coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "test": "echo 'hello world'",
    "testw": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "eslint ./src/**/*.ts --fix && prettier . --write",
    "release": "rm -rf ./build && npm run build && npm publish --access public"
  },
  "files": [
    "build/",
    "!**/*.tsbuildinfo"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.16.0",
