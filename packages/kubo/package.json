{
  "name": "@zkdb/kubo",
  "private": false,
  "version": "1.0.5",
  "description": "zkDatabase Kubo RPC client",
  "type": "module",
  "types": "./build/src/index.d.ts",
  "scripts": {
    "release": "rm -rf ./build && npm run build && npm publish --access public",
    "build": "tsc",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orochi-network/zkDatabase.git"
  },
  "keywords": [
    "zkdb",
    "zkdatabase",
    "kubo",
    "rpc",
    "ipfs",
    "client"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "build/src/*"
      ]
    }
  },
  "files": [
    "build/src",
    "!**/*.tsbuildinfo",
    "!**/*.test.js",
    "!**/*.test.js.map",
    "!**/*.test.d.ts",
    "!**/*.test.d.ts.map"
  ],
  "exports": {
    ".": {
      "import": "./build/src/index.js",
      "types": "./build/src/index.d.ts"
    }
  },
  "author": "chiro@orochi.network",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/orochi-network/zkDatabase/issues"
  },
  "homepage": "https://github.com/orochi-network/zkDatabase#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.8",
    "axios-mock-adapter": "^1.22.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
